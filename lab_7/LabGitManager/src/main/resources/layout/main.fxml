<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <ToolBar AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <items>
            <Button fx:id="addGroupButton" text="Add group" />
            <Button fx:id="addRepoButton" maxWidth="300.0" text="Add repository" />
        </items>
    </ToolBar>
   <TreeTableView fx:id="groupsTable" layoutY="78.0" prefHeight="300.0" prefWidth="600" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="74.0">
   </TreeTableView>
   <ToolBar layoutY="38.0" nodeOrientation="LEFT_TO_RIGHT" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="38.0">
     <items>
            <Button fx:id="showStatisticsButton" text="Show statistics" />
            <Button fx:id="executeButton" text="Execute for a group" />
     </items>
   </ToolBar>

</AnchorPane>
